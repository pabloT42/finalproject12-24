#include <iostream>
using namespace std;
bool hasWon = false;

//global variable



string inventory[5];


int playerhealth = 100;


void shop();

void BattleSystem();
int monstergen();


//definition
void shop() {
	char input = 'a';
	cout << endl << endl << "-------------------------------------" << endl;
	cout << "welcome to the shop! press L to quit" << endl;
	while (input != 'l') {
		cout << "pick an item: s) sword, g) armor" << endl;
		cin >> input;
		switch (input) {
		case 'g':
			cout << "ok heres your protective gear!" << endl;
			inventory[1] = "armor";
			break;
		case 's':
			cout << "ok heres your sword!" << endl;
			inventory[0] = "sword";
			break;
		
		}

	}
	cout << "------------------------------------" << endl;
}




//definition
void bakery() {
	char input = 'a';
	cout << endl << endl << "-------------------------------------" << endl;
	cout << "welcome to the bakery! press L to quit" << endl;
	while (input != 'l') {
		cout << "pick an item: b) bread, c) cake, p)pudding  " << endl;
		cin >> input;
		switch (input) {
		case 'b':
			cout << "ok heres your yummy bread!" << endl;
			inventory[2] = "bread";
			break;
		case 'c':
			cout << "ok heres your cake!" << endl;
			inventory[3] = "cake";
			break;
		case 'p':
			cout << "take this pudding made by pompompurin" << endl;
			inventory[4] = "pudding";
				break; 

		}

	}
	cout << "------------------------------------" << endl;
}



//function declaration
void BattleSystem() {
	int MonsterType = monstergen();


	int monsterhealth = MonsterType * 10;
	int hit;
	int choice;

	cout << endl << "---------------BATTLE!------------------" << endl;
	while (playerhealth > 0 && monsterhealth > 0) {



		//player attack sequence
		if (inventory[0] == "sword") {
			hit = rand() % 11 + 10; //swing for 10-20 hp
			cout << "you slash the monster with your sword for " << hit << "dmg" << endl;
			monsterhealth -= hit;
		}
		else {
			hit = rand() % 5 + 2;
			cout << "you punch the monster for " << hit << "dmg " << endl;
		}
		// monster attack sequence
		if (inventory[1] == "armor") {
			hit = rand() % 5 + 2 * MonsterType; //swing for 10-20 hp
			cout << "the armor softens the blow and you only took " << hit << "dmg" << endl;
			playerhealth -= hit;
		}
		else {
			hit = rand() % 15 + 12;
			cout << "monster bites in to you for " << hit << "dmg " << endl;
			playerhealth -= hit;
		}



		cout << "player health is now" << playerhealth << endl;
		cout << "monster health is now " << monsterhealth << endl;
	}//end of loop

	cout << "-------------end of battle!---------------------------" << endl;
}//end of function 

//monster gen
int monstergen() {


	int num = rand() % 100 + 1;


	if (num < 45) {
		cout << "a blobby appeared!" << endl;
		return 1;
	}
	else if (num < 60) {
		cout << "a pidgy pops up" << endl;
		return 2;
	}
	else if (num < 75) {
		cout << "a zombob attacks you" << endl;
		return 3;
	}
	else if (num < 95) {
		cout << "look out a dwarf!!!!!!!!!!!! " << endl;
		return 4;
	}
	else {
		cout << "oh no its an evil horsey" << endl;
		return 5;
	}
	return 0;

}





